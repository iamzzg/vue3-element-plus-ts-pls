{
  "name": "vite-vue3-ts-demo",
  "version": "0.0.0",
  "private": true,
  "author": {
    "name": "dragon",
    "email": "785864219@qq.com"
  },
  "scripts": {
    "dev": "vite",
    "boostrap": "pnpm install",
    "build": "cross-env NODE_ENV=production vite build && esno ./build/script/postBuild.ts",
    "build:report": "cross-env REPORT=true pnpm build",
    "build:type-check": "run-p type-check build",
    "commit": "git-cz",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "lint:eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:prettier": "prettier --write src/",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged",
    "new": "plop",
    "preview": "vite preview",
    "prepare": "husky install",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf package-lock.json && rimraf yarn.lock && rimraf node_modules && npm run boostrap",
    "svgo": "svgo -f src/assets/svgs -o src/assets/icons --config=src/assets/svgo.config.js",
    "type-check": "vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@vueuse/core": "^10.1.2",
    "axios": "^1.4.0",
    "dayjs": "^1.11.7",
    "element-plus": "^2.3.4",
    "pinia": "^2.0.32",
    "vue": "^3.3.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.16.5",
    "@vitejs/plugin-legacy": "^4.0.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue-macros/reactivity-transform": "^0.3.5",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.14",
    "chalk": "^5.2.0",
    "commitizen": "^4.3.0",
    "commitlint": "^17.6.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-git": "^1.6.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "plop": "^3.1.2",
    "postcss": "^8.4.23",
    "postcss-html": "1.4.0",
    "postcss-scss": "3.0.0",
    "prettier": "^2.8.4",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.62.1",
    "stylelint": "^14.7.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "3.1.0",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "svgo": "^3.0.2",
    "tailwindcss": "^3.3.2",
    "typescript": "~4.8.4",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.5",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^1.6.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  }
}
